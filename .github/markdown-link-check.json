{
  "ignorePatterns": [
    {
      "pattern": "^https://pypi.org/project/easecloud-errica"
    },
    {
      "pattern": "^https://test.pypi.org/project/easecloud-errica"
    },
    {
      "pattern": "^https://easecloud-errica.readthedocs.io"
    },
    {
      "pattern": "^mailto:"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}